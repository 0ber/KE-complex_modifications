{
    "title": "German Umlaut",
    "rules": [
        {
            "description": "Change option + e/o/u to ë/ö/ü",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("e", ["option"], ["caps_lock"]) %>,
                    "to": <%= to([["u", ["left_option"]], ["e"], ["vk_none"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("e", ["option", "shift"], []) %>,
                    "to": <%= to([["u", ["left_option"]], ["e", ["left_shift"]], ["vk_none"]]) %>
                },

                {
                    "type": "basic",
                    "from": <%= from("o", ["option"], ["caps_lock"]) %>,
                    "to": <%= to([["u", ["left_option"]], ["o"], ["vk_none"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("o", ["option", "shift"], []) %>,
                    "to": <%= to([["u", ["left_option"]], ["o", ["left_shift"]], ["vk_none"]]) %>
                },

                {
                    "type": "basic",
                    "from": <%= from("u", ["option"], ["caps_lock"]) %>,
                    "to": <%= to([["u", ["left_option"]], ["u"], ["vk_none"]]) %>
                },
                {
                    "type": "basic",
                    "from": <%= from("u", ["option", "shift"], []) %>,
                    "to": <%= to([["u", ["left_option"]], ["u", ["left_shift"]], ["vk_none"]]) %>
                }
            ]
        }
    ]
}
