{
  "title": "Personal rules (@tekezo)",
  "rules": [
    {
      "description": "Change control+1/2/v/q to home/end/page_down/page_up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ]
        }
      ]
    },
    {
      "description": "Extra emacs key bindings",
      "manipulators": [
        {
          "description": "Change control+u to delete-to-beginning-of-line",
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": [
                "left_control",
                "left_shift"
              ]
            },
            {
              "key_code": "delete_or_backspace"
            },
            {
              "key_code": "vk_none"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^org\\.gnu\\.Emacs$",
                "^org\\.gnu\\.AquamacsEmacs$",
                "^org\\.gnu\\.Aquamacs$",
                "^org\\.pqrs\\.unknownapp.conkeror$",
                "^com\\.apple\\.Terminal$",
                "^com\\.googlecode\\.iterm2$",
                "^co\\.zeit\\.hyperterm$",
                "^co\\.zeit\\.hyper$"
              ]
            }
          ]
        },
        {
          "description": "Change control+w to option+delete",
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "control"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^org\\.gnu\\.Emacs$",
                "^org\\.gnu\\.AquamacsEmacs$",
                "^org\\.gnu\\.Aquamacs$",
                "^org\\.pqrs\\.unknownapp.conkeror$",
                "^com\\.apple\\.Terminal$",
                "^com\\.googlecode\\.iterm2$",
                "^co\\.zeit\\.hyperterm$",
                "^co\\.zeit\\.hyper$"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Change tab to command+`",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    },
    {
